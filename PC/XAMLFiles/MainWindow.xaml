<Window x:Class="PC.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PC"
        xmlns:portpresenter="clr-namespace:PC.XAMLFiles"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="800">
    <Window.DataContext>
        <local:PresentViewModel></local:PresentViewModel>
    </Window.DataContext>
    <Grid>
        <DockPanel>
            <portpresenter:PortPresenter DockPanel.Dock="Top"></portpresenter:PortPresenter>
            <portpresenter:ToolbarPresenterLegacy DockPanel.Dock="Left"></portpresenter:ToolbarPresenterLegacy>
            <portpresenter:ConsolePresenter DockPanel.Dock="Bottom" Height="220" MaxHeight="220"></portpresenter:ConsolePresenter>
            <portpresenter:PlotPresenter></portpresenter:PlotPresenter>
        </DockPanel>
    </Grid>
</Window>
